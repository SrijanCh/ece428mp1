package main
import(
		"fmt"
	    "net/rpc"
	    "log"
		"querier"
	  )

func main(){
	client, err := rpc.DialHTTP("tcp", "localhost" + ":1234")
	if err != nil {log.Fatal("dialing:", err)}
	// Synchronous call
	args := &querier.Args{"He","machine.i.log"}
	fmt.Printf("Args{Data = %s, Filepath = %s}\n", args.Data, args.Filepath)
	var reply string
	err = client.Call("Querier.Grep", args, &reply)
	if err != nil {
		fmt.Printf("WE FUCKED UP\n")
		log.Fatal("query error: ", err)
	}
	fmt.Printf("grep result: %s\n", reply)
}
